# MMH v3.1 Environment Variables
# Copy to .env and fill in your values

# General
MMH_INSTANCE_ID=mmh-node-1
MMH_ENVIRONMENT=production
MMH_DRY_RUN=true

# Redis
MMH_REDIS_URL=redis://localhost:6379

# PostgreSQL
MMH_POSTGRES_DSN=postgresql://mmh:mmh@localhost:5432/mmh

# Solana
MMH_SOLANA_RPC_URL=
MMH_SOLANA_WS_URL=
MMH_HELIUS_API_KEY=
MMH_HELIUS_WS_URL=
MMH_BIRDEYE_API_KEY=
MMH_PUMPPORTAL_WS_URL=wss://pumpportal.fun/api/data

# Base/EVM
MMH_BASE_RPC_URL=
MMH_BASE_WS_URL=
MMH_ALCHEMY_API_KEY=
MMH_ZEROX_API_KEY=

# Telegram
MMH_TELEGRAM_BOT_TOKEN=
MMH_TELEGRAM_CHAT_ID=

# Risk
MMH_MAX_PORTFOLIO_EXPOSURE_USD=500.0
MMH_DAILY_LOSS_LIMIT_USD=200.0

# Observability
MMH_LOG_LEVEL=INFO
MMH_LOG_FORMAT=json
MMH_PROMETHEUS_PORT=8000
